{
  "name": "@luma.gl/text",
  "version": "9.2.0-alpha.6",
  "description": "3D text generation utilities for luma.gl",
  "license": "MIT",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/visgl/luma.gl"
  },
  "keywords": [
    "webgl",
    "visualization",
    "animation",
    "3d",
    "text"
  ],
  "types": "dist/index.d.ts",
  "main": "dist/index.cjs",

    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "src",
    "dist",
    "dist.min.js",
    "README.md"
  ],
  "sideEffects": false,
  "dependencies": {
    "@math.gl/core": "^4.1.0",
    "@luma.gl/engine": "9.2.0-alpha.6",
    "@math.gl/polygon": "^4.1.0"
  }
}

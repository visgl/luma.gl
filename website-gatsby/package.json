{
  "name": "luma.gl-website",
  "version": "0.0.0",
  "description": "A website for luma.gl built with Ocular (Gatsby version)",
  "private": true,
  "license": "MIT",
  "keywords": [
    "ocular"
  ],
  "workspaces": [
    "react"
  ],  
  "main": "index.js",
  "scripts": {
    "start": "yarn clean-examples && yarn clean && yarn develop",
    "clean": "rm -rf ./.cache ./public",
    "develop": "gatsby develop --port=8001",
    "build": "yarn clean-examples && yarn clean && gatsby build",
    "build-staging": "yarn clean-examples && yarn clean && gatsby build --prefix-paths",
    "serve": "gatsby serve",
    "deploy": "gh-pages -d public",
    "clean-examples": "rm -rf examples ; (find ../examples -name node_modules -exec rm -rf {} \\; || true)"
  },
  "dependencies": {
    "@loaders.gl/polyfills": "^3.2.12",
    "@probe.gl/stats-widget": "^4.0.0",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "mapbox-gl": "^1.2.1"
  },
  "devDependencies": {
    "gatsby": "^2.29.3",
    "gatsby-theme-ocular": "^1.2.4",
    "gatsby-plugin-no-sourcemaps": "^2.1.2",
    "gh-pages": "^2.2.0"
  },
  "resolutions": {
    "remark-mdx": "1.6.22"
  }
}

{
  "name": "@luma.gl/gltools",
  "version": "9.0.0-alpha.27",
  "description": "WebGL2 API Polyfills for WebGL1 WebGLRenderingContext",
  "type": "module",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/visgl/luma.gl"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "webgl2",
    "webgl",
    "polyfill"
  ],
  "types": "dist/index.d.ts",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "src",
    "dist",
    "dist.min.js",
    "README.md"
  ],
  "scripts": {
    "build-bundle": "ocular-bundle ./src/index.ts",
    "pre-build": "npm run build-bundle && npm run build-bundle -- --env=dev"
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0",
    "@luma.gl/constants": "9.0.0-alpha.27",
    "@luma.gl/engine": "9.0.0-alpha.27",
    "@luma.gl/webgl": "9.0.0-alpha.27",
    "@probe.gl/log": "^4.0.2"
  },
  "gitHead": "c636c34b8f1581eed163e94543a8eb1f4382ba8e"
}

  },

  "GL.TEXTURE_COMPARE_FUNC": {
    "title": "Texture Comparison Function",

    "GL.LEQUAL": {"default": true, "d": "result = 1.0 0.0, r <= D t r > D t"},
    "GL.GEQUAL":   "result = 1.0 0.0, r >= D t r < D t",
